<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="305.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.mytunes.gui.NewSongController">
   <children>
      <Label layoutX="152.0" layoutY="14.0" text="New/Edit song">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox alignment="TOP_CENTER" layoutX="2.0" layoutY="52.0" prefHeight="191.0" prefWidth="91.0">
         <children>
            <Label text="Title:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="Artist">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="Category">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="Time">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="File">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <TextField fx:id="titleTextField" layoutX="93.0" layoutY="52.0" />
      <TextField fx:id="artistTextField" layoutX="93.0" layoutY="83.0" />
      <TextField fx:id="timeTextField" layoutX="93.0" layoutY="148.0" />
      <TextField fx:id="filePathTextField" layoutX="93.0" layoutY="180.0" />
      <ComboBox fx:id="categoryComboBox" layoutX="93.0" layoutY="116.0" prefWidth="150.0" />
      <Button layoutX="257.0" layoutY="116.0" mnemonicParsing="false" onAction="#onAddCategoryMoreClick" text="More..." />
      <Button fx:id="chooseButton" layoutX="258.0" layoutY="180.0" mnemonicParsing="false" onAction="#chooseSongButtonClick" text="Choose..." />
      <Button fx:id="cancelButton" layoutX="264.0" layoutY="254.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
      <Button fx:id="saveButton" layoutX="331.0" layoutY="254.0" mnemonicParsing="false" onAction="#onAddSongButtonClick" text="Save" />
   </children>
</AnchorPane>
